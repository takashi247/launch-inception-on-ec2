{
    "listeners": {
      "*:${PORT_NUM_ADMINER}": {
        "pass": "routes"
      }
    },
    "routes": [
      {
        "match": {
          "uri": ["/adminer.css"]
        },
        "action": {
          "share": "/var/www/html/"
        }
      },
      {
        "action": {
          "pass": "applications/adminer"
        }
      }
    ],
    "applications": {
      "adminer": {
        "type": "php",
        "root": "/var/www/html/",
        "index": "adminer.php"
      }
    }
  }